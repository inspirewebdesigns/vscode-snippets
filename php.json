			"  \"${5:values}\" => array(",
			"    \"${6:ENUM}\" => array(",
			"      \"value\" => \"${7:value}\"",
			"      )",
			"  )",
			"));"
		],
		"description": ""
	},
	"register_graphql_union_type": {
		"prefix": "register_graphql_union_type",
		"body": [
			"register_graphql_union_type(\"${1:TypeName}\", array(",
			"  \"typeNames\" => array(\"${2:String}\"),",
			"  \"${3:resolveType}\" => function ($${4:object}) {",
			"    $$type = null;",
			"    switch ($${4:object}[\"type\"]) {",
			"      case \"${5:Type}\":",
			"        break;",
			"    }",
			"    return $$type;",
			"  }",
			"));"
		],
		"description": ""
	},
