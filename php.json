	"register_graphql_object_type": {
		"prefix": "register_graphql_object_type",
		"body": [
			"register_graphql_object_type(\"${1:TypeName}\", array(",
			"  \"description\" => __(\"${2:description}\", \"${3:slug}\"),",
			"  \"fields\" => array(",
			"    \"${4:name}\" => array(",
			"      \"type\" => \"${5:String}\",",
			"      \"description\" => __(\"${6:description}\", \"${3:slug}\")",
			"    )",
			"  )",
			"));"
		],
		"description": ""
	},
	"register_graphql_field": {
		"prefix": "register_graphql_field",
		"body": [
			"register_graphql_field(",
			"  \"${1:SchemaType}\",",
			"  \"${2:queryName}\",",
			"  array(",
			"    \"type\" => \"${3:String}\",",
			"    \"description\" => __(\"${4:description}\", \"${5:slug}\"),",
			"    \"resolve\" => function ($${6:source}, $${7:args}, $${8:info}, $${9:context}) {",
			"    }",
			"  )",
			");"
		],
		"description": ""
	},
	"register_graphql_enum_type": {
